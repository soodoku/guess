citHeader("To cite package 'guess' in publications use:")
     
year <- sub(".*(2[[:digit:]]{3})-.*", "\\1", meta$Date, perl = TRUE)
vers <- paste("R package version", meta$Version)
 
citEntry(entry="Manual",
		  title = "guess: Estimates of Learning Adjusted for Guessing.",
		  author = personList(as.person("Gaurav SOod")),
		  year = year,
		  note = vers,
		  textVersion =
		  paste("Gaurav Sood (",
				year,
				"). guess: Estimates of Learning Adjusted for Guessing. ",
				vers, ".", sep=""))